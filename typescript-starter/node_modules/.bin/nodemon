#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/bin/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/nodemon@2.0.7/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/node_modules:/home/jigth/.MyCourses/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/nodemon/bin/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/nodemon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
