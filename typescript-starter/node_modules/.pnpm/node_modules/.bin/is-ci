#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/is-ci@2.0.0/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/node_modules:/home/jigth/.MyCourses/node_modules:/home/jigth/node_modules:/home/node_modules:/node_modules:/home/jigth/.MyCourses/SystemsArchitecture/typescript-starter/node_modules/.pnpm/node_modules/is-ci/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-ci/bin.js" "$@"
else
  exec node  "$basedir/../is-ci/bin.js" "$@"
fi
